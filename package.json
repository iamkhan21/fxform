{
  "name": "@iamkhan21/fxform",
  "version": "2.2.0",
  "description": "Reactive forms based on effector and zod",
  "keywords": [
    "effector",
    "forms"
  ],
  "author": {
    "name": "Ivan Khanevich",
    "email": "mail@iamkhan.tech"
  },
  "repository": "https://github.com/iamkhan21/fxform",
  "license": "Apache-2.0",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "release": "standard-version && git push --follow-tags origin master && pnpm run publish",
    "prepublishOnly": "rimraf ./dist && rimraf ./package && npm run build",
    "publish": "clean-publish",
    "postpublish": "rimraf ./dist && rimraf ./package",
    "update:deps": "pnpm up -r -i -L && pnpm i"
  },
  "dependencies": {
    "effector": "22.4.0",
    "patronum": "1.12.3",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.8.0",
    "@parcel/transformer-typescript-types": "2.8.0",
    "clean-publish": "4.0.1",
    "parcel": "2.8.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "standard-version": "9.5.0",
    "typescript": "4.8.4"
  },
  "clean-publish": {
    "access": "public",
    "files": [
      ".idea",
      ".parcel-cache",
      "src",
      "tsconfig.json"
    ],
    "tempDir": "package",
    "packageManager": "pnpm"
  }
}
